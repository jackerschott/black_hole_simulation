#version 320 es

#include "float.glsl"
#include "complex.glsl"
#include "elliptic.glsl"

layout(location = 0)uniform vec2 m;

layout(binding = 0) buffer phiBuf { vec2 phi[]; };
layout(binding = 1) buffer resBuf { vec2 res[]; };

layout(local_size_x = 1) in;
void main() {
  uint g = gl_GlobalInvocationID.x;
  res[g] = elliptic_f(phi[g], m);
}
